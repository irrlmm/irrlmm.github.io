---
import { getEntry } from "astro:content";
import QuizStack from "./QuizStack";
import type { ReferenceDataEntry } from "astro:content";
interface Props {
  quiz: ReferenceDataEntry<"quiz">;
}

const { quiz } = Astro.props;

const q = await getEntry("quiz", quiz.id);

if (!q) {
  throw Error("Quiz not found!");
}
---

<QuizStack client:idle data={q.data} />
